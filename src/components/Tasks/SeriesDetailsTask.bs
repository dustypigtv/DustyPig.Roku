Sub Init()

    print "SeriesDetailsTask:Init"

    m.top.functionName = "RunTask"

    End Sub

Sub RunTask()

    print "SeriesDetailsTask:RunTask"

    id = m.top.id
    url = `/Series/Details/${id}`

    result = GetData(url, m.global.profileToken)

    if result.statusCode = 401 then
        m.global.loggedOut = true
        return
    end if

    m.top.data = result

End Sub